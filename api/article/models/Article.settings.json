{
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "article",
    "description": "Article posts "
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "Title": {
      "default": "",
      "type": "string"
    },
    "Content": {
      "default": "",
      "type": "text"
    },
    "Image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "users": {
      "collection": "user",
      "via": "article",
      "plugin": "users-permissions"
    }
  }
}